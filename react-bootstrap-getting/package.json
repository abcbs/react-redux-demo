  "scripts": {
    "build-cli":"npm run start-dev",
    "start-dev": "better-npm-run abc:build-cli",
    "start-run": "better-npm-run abc:run",
    "start": "webpack-dev-server --config webpack.config.js --progress --hot --colors",
    "build": "webpack --config webpack.config.js --progress --colors",
    "ddl": "webpack --config webpack.config.vendors",
    "ddlserver": "webpack-dev-server --config webpack.config.server --progress  --hot --colors ",
    "ddlpro": "webpack-dev-server --config webpack.config.js --progress  --colors ",
    "nstart": "node server.js",
    "ddluser": "webpack --config webpack.config.dll.users",
    "ddldev": "webpack --config webpack.config.dev",
    "release": "gulp abc:release --vendmodel debug --abcmodel debug",
    "debug": "gulp",
    "mtest": "mocha --compilers js:babel-register  --recursive",
    "test:watch": "npm test -- --watch",
    "jtest": "jest",
    "ktest": "node_modules/.bin/karma start karma.config.js"
  },
  "betterScripts": {
    "abc:build-cli": {
      "command": "gulp abc:build-cli --vendmodel debug --abcmodel debug",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "abc:run": {
      "command": "node ./lib/es/todos/renders/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3000,
        "HOST": "192.168.1.102",
        "APIHOST": "192.168.1.102",
        "APIPORT": 3030
      }
    }
  },
